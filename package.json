  "main": "src/index.tsx",
  "scripts": {
    "develop": "run-p webpack:serve typed:scss:watch",
    "build": "run-s typed:scss:build webpack:build",
    "start": "yarn develop",
    "webpack:serve": "cross-env DEV_SERVER=true webpack serve",
    "webpack:build": "yarn ts:webpack --mode=production",
    "code:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md,html}\"",
    "test": "yarn test:depcruise",
    "test:depcruise": "depcruise ./src",
    "typed:scss:build": "tsm \"src/**/*.scss\" --implementation \"sass\" --aliasPrefixes.~ node_modules/",
    "typed:scss:watch": "yarn typed:scss:build --watch",
    "ts": "ts-node -P tsconfig.script.json",
    "check:pr": "yarn ts ./scripts/check-pr",
    "ts:webpack": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "check:merge": "yarn ts ./scripts/check-merge"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "bootstrap": "^5.0.1",
    "mobx": "^6.3.2",
